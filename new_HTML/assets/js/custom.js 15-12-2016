angular.module('shoutModule').controller('Modaleditdiscussion', function ($scope, $uibModal, $log) {
  $scope.items = ['item1', 'item2', 'item3'];
  $scope.animationsEnabled = true;
  $scope.openeditdiscussion = function (size) {
    var modalInstance = $uibModal.open({
      animation: $scope.animationsEnabled,
