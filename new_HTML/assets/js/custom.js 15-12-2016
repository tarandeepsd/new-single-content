// Top Navigation Scroll fixed
////////////////////////////////////////////////////////////////////////////
angular.module('shoutModule').directive("scroll", function ($window) {
    return function(scope, element, attrs) {
        angular.element($window).bind("scroll", function() {
             if (this.pageYOffset >= 60) {
                 element.addClass('scroll-bg');
                 console.log('Scrolled below header.');
             } else {
                 element.removeClass('scroll-bg');
                 console.log('Header is in view.');
             }
        });
    };
});
angular.module('shoutModule').directive("toggleChecked", function ($window) {
    return function(scope, element, attrs) {
		console.log(element);
        angular.element(element).on("click", function() {
             console.log($(this).parents('div#toggleChecked').toggleClass('pod_active'));
             // console.log($(this).parents('div#toggleChecked').toggleChecked());
        });
    };
});

