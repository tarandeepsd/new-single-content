//ModalProfileCtrl
angular.module('shoutModule').controller('ModalProfileCtrl', function ($scope, $uibModal, $log) {
  $scope.items = ['item1', 'item2', 'item3'];
  $scope.animationsEnabled = true;
  $scope.emailBlur = false;
  $scope.passBlur = false;
  $scope.isCollapsed= true;
  $scope.isemailCollapsed= true;
  $scope.ispassCollapsed= true;
  $scope.isnoCollapsed= true;
  $scope.isageCollapsed= true;
  $scope.isgenderCollapsed= true;
  $scope.isfamilyCollapsed= true;
  $scope.open = function (size) {
    var modalInstance = $uibModal.open({
      animation: $scope.animationsEnabled,
      templateUrl: 'myModalContent.html',
      controller: 'ModalInstanceCtrl',
      size: size,
      resolve: {
        items: function () {
          return $scope.items;
        }
      }
    });
    modalInstance.result.then(function (selectedItem) {
    $scope.selected = selectedItem;
    }, function () {
    $scope.isCollapsed= true;
    $scope.isemailCollapsed= true;
    $scope.ispassCollapsed= true;
    $scope.isnoCollapsed= true;
    $scope.isageCollapsed= true;
    $scope.isgenderCollapsed= true;
    $scope.isfamilyCollapsed= true;
      $log.info('Modal dismissed at: ' + new Date());
    });
  };
  $scope.toggleAnimation = function () {
    $scope.animationsEnabled = !$scope.animationsEnabled;
  };
});
//ModalProfileCtrl

//ModalProfileCtrl
angular.module('shoutModule').controller('ModalGeneralCtrl', function ($scope, $uibModal, $log) {
  $scope.items = ['item1', 'item2', 'item3'];
  $scope.animationsEnabled = true;
  $scope.emailBlur = false;
  $scope.passBlur = false;
  $scope.isCollapsed= true;
  $scope.isemailCollapsed= true;
  $scope.ispassCollapsed= true;
  $scope.isnoCollapsed= true;
  $scope.isageCollapsed= true;
  $scope.isgenderCollapsed= true;
  $scope.isfamilyCollapsed= true;
  $scope.open = function (size) {
    var modalInstance = $uibModal.open({
      animation: $scope.animationsEnabled,
      templateUrl: 'myModalContent.html',
      controller: 'ModalInstanceCtrl',
      size: size,
      resolve: {
        items: function () {
          return $scope.items;
        }
      }
    });
    modalInstance.result.then(function (selectedItem) {
    $scope.selected = selectedItem;
    }, function () {
    $scope.isCollapsed= true;
    $scope.isemailCollapsed= true;
    $scope.ispassCollapsed= true;
    $scope.isnoCollapsed= true;
    $scope.isageCollapsed= true;
    $scope.isgenderCollapsed= true;
    $scope.isfamilyCollapsed= true;
      $log.info('Modal dismissed at: ' + new Date());
    });
  };
  $scope.toggleAnimation = function () {
    $scope.animationsEnabled = !$scope.animationsEnabled;
  };
});
