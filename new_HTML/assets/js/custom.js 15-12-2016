angular.module('shoutModule').directive("filterToggle", function () {
    return {
        scope: true,
	controller: function( $scope ) {
	  $scope.isCollapsed = true;
	},
	link: function(scope, element){
	  scope.isCollapsed = true;
	}
    };
});
angular.module('shoutModule').controller('mmenuCtrl', function ($scope) {
  $scope.closemmenu = function(){
    $("#menu").data("mmenu").close();
  }
  $scope.closemmenubyheader = function(){
	if($('#menu').hasClass('mm-opened')) {
	$("#menu").data("mmenu").close();
	}
  }
});


angular.module('shoutModule').directive('mmenu', function() {
  return {
    restrict : 'A',
	controller : "mmenuCtrl",
    link : function(scope, element, attrs) {
            $(element).mmenu({
              "extensions": [
                  "pagedim-black"
               ],
              offCanvas: {
                 position  : "left",
                 zposition : "front"
              },
			  navbar:{
				title:"<a href='deal-category.html' class='mm-title'>All Category</a>"  
			   }
			  
            });
    }
  };
});
angular.module('shoutModule').directive("filterToggleReset", function () {
    return {
        require: '?^filterToggle',
      	link: function(scope, element, attr){
          scope.model = {
            second: [],
            discount: [],
            dbrange:[]
          };
      	  element.on('click', function( event ){
            event.stopPropagation();
            var minVal = parseFloat(attr.min);
            var maxVal = parseFloat(attr.max);
            var stype = attr.slidertype;
            if(stype == 'priceRange'){
              scope.$apply(function(){
                scope.price = {
                  from: minVal,
                  to:maxVal
                }
              });
            }
            if(stype == 'dbRange'){
              scope.$apply(function(){
                scope.db = {
                  from: minVal,
                  to:maxVal
                }
              });
            }
             if(stype == 'discountRange'){
              scope.$apply(function(){
                scope.discount = {
                  from: minVal,
                  to:maxVal
                }
              });
            }
      	  });
      	}
    };
});



