/** Favourite cotegories Modal **/
angular.module('shoutModule').controller('ModalNewDealPresentOnline', function ($scope, $uibModal, $log, $timeout, $document) {
  $scope.items = ['item1', 'item2', 'item3'];
  $scope.animationsEnabled = true;
  $scope.openNewDealPresentOnline = function (size) {
    var modalInstance = $uibModal.open({
      animation: $scope.animationsEnabled,
      templateUrl: 'modal/newdeal-present-online-modal.html',
      controller: 'ModalInstancedealCtrl',
      size: size,
	  windowClass:"modal-editdeal",
      resolve: {
        items: function () {
          return $scope.items;
	   },
      }
    });
    $scope.modalInstance = modalInstance;
    modalInstance.result.then(function (selectedItem) {
      $scope.selected = selectedItem;
    }, function () {
      $log.info('Modal dismissed at: ' + new Date());
    });
  };
  $scope.toggleAnimation = function () {
    $scope.animationsEnabled = !$scope.animationsEnabled;
  };
});


angular.module('shoutModule').controller('ModalInstancedealCtrl', ['$scope', '$uibModalInstance', 'items', '$timeout', '$document',function ($scope, $uibModalInstance, items, $timeout, $document) {
  $scope.items = items;
  $timeout(function(){
	  $scope.$emit('content.changed');
	},0);
  $scope.selected = {
    item: $scope.items[0]
  };
  $scope.step_1 = true;
  $scope.step_2 = false;
  $scope.step_3 = false;
  $scope.step_4 = false;
  $scope.step_5 = false;
  $scope.step_6 = false;
  $scope.ok = function () {
    $uibModalInstance.close($scope.selected.item);
  };
 $scope.cancel = function () {
    $uibModalInstance.dismiss('cancel');
  };
  $scope.step2 = function(className ){
	$scope.step_1 = false;
    $scope.step_2 = true;
 	setTimeout(function(){$scope.$apply()},1)	
  }

  
  $scope.step3 = function( className ){
    $scope.step_2 = false;
    $scope.step_3 = true;
 	setTimeout(function(){$scope.$apply()},1)	
  }

  $scope.step4 = function( className ){
    $scope.step_3 = false;
    $scope.step_4 = true;
    angular.element( document.getElementsByClassName("modal-shoutnewdeal") ).addClass(className);
  }

  $scope.step5 = function( className ){
    $scope.step_4 = false;
    $scope.step_5 = true;
    angular.element( document.getElementsByClassName("modal-shoutnewdeal") ).addClass(className);
  }

  $scope.step6 = function( className ){
    $scope.step_5 = false;
    $scope.step_6 = true;
    angular.element( document.getElementsByClassName("modal-shoutnewdeal") ).addClass(className);
  }
  
  //========================================== DATEPICKER START
  
  		$scope.formData = {};
  
		$scope.formData.fromDate1 = new Date(); //  new Date(items.description.appointment_date);
		$scope.formData.fromDate2 = new Date();

		$scope.today = function() {
		  $scope.formData.fromDate1 = new Date();
		  $scope.formData.fromDate2 = new Date();
		};
		$scope.today();
		
		$scope.clear = function() {
		  $scope.formData.fromDate1 = null;
		  $scope.formData.fromDate1 = null;
		};
		
		
		$scope.minDate = new Date();
		
		$scope.dateOptions = {
		  //minDate: new Date(),
		  showWeeks: false,
		  startingDay: 0
		};
		
		$scope.open1 = function() {
		  $scope.popup1.opened = true;
		};
		$scope.open2 = function() {
		  $scope.popup2.opened = true;
		};
		$scope.open3 = function() {
			console.log("hey im here")
		  $scope.popup3.opened = true;
		};
		
		$scope.setDate = function(year, month, day) {
		  $scope.formData.fromDate = new Date(year, month, day);
		};
		
		$scope.formats = ['dd-MMMM-yyyy', 'yyyy/MM/dd', 'dd.MM.yyyy', 'shortDate', 'dd/MM/yyyy'];
		$scope.format = $scope.formats[4];
		$scope.altInputFormats = ['M!/d!/yyyy'];
		
		$scope.popup1 = {
		  opened: false
		};
		$scope.popup2 = {
		  opened: false
		};
		$scope.popup3 = {
		  opened: false
		};
	//datepicker end===============================================================
  
}]);


