angular.module('shoutModule').controller('ModalInstancesmCtrl', function ($scope, $uibModalInstance, items) {

	console.log("I'm here int his controller");

  $scope.items = items;
  $scope.selected = {
    item: $scope.items[0]
  };
  $scope.ok = function () {
    $uibModalInstance.close($scope.selected.item);
  };
  $scope.cancel = function () {
    $uibModalInstance.dismiss('cancel');
  };
   $scope.step2 = function( className ){
	angular.element( document.getElementsByClassName("modal-upload-image") ).removeClass("upload-cover");
  }
   $scope.step3 = function( className ){
		angular.element( document.getElementsByClassName("modal-upload-image") ).addClass("upload-cover");
  }

 
  
});

/** Favourite cotegories Modal **/
angular.module('shoutModule').controller('ModalDemobigCtrl', function ($scope, $uibModal, $log, $timeout, $document) {
  $scope.items = ['item1', 'item2', 'item3'];
  $scope.animationsEnabled = true;
  $scope.openbig = function (size) {
    var modalInstance = $uibModal.open({
      animation: $scope.animationsEnabled,
      templateUrl: 'modal/favourite-category.html',
      controller: 'ModalInstancebigCtrl',
      size: size,
      resolve: {
        items: function () {
          return $scope.items;
		  
        },
      }
    });
    $scope.modalInstance = modalInstance;
    modalInstance.result.then(function (selectedItem) {
      $scope.selected = selectedItem;
    }, function () {
      $log.info('Modal dismissed at: ' + new Date());
    });

  };
});



angular.module('shoutModule').controller('ModalInstancebigCtrl', ['$scope', '$uibModalInstance', 'items', '$timeout', '$document',function ($scope, $uibModalInstance, items, $timeout, $document) {
  $scope.items = items;
  $timeout(function(){
	  $scope.$emit('content.changed');
	},0);
  $scope.selected = {
    item: $scope.items[0]
  };
  $scope.step_1 = true;
  $scope.ok = function () {
    $uibModalInstance.close($scope.selected.item);
  };


  $scope.cancel = function () {
    $uibModalInstance.dismiss('cancel');
  };

  $scope.step2 = function(  className ){
    $scope.step_1 = false;
    $scope.step_2 = true;
    angular.element( document.getElementsByClassName("modal-big") ).addClass(className);
  }

  
  $scope.step3 = function( className ){
    $scope.step_2 = false;
    $scope.step_3 = true;
    angular.element( document.getElementsByClassName("modal-big") ).addClass(className);
  }

  $scope.step4 = function( className ){
    $scope.step_3 = false;
    $scope.step_4 = true;
    angular.element( document.getElementsByClassName("modal-big") ).addClass(className);
  }

  $scope.step5 = function( className ){
    $scope.step_4 = false;
    $scope.step_5 = true;
    angular.element( document.getElementsByClassName("modal-big") ).addClass(className);
  }

  $scope.step6 = function( className ){
    $scope.step_5 = false;
    $scope.step_6 = true;
    angular.element( document.getElementsByClassName("modal-big") ).addClass(className);
  }
}]);
