// Coupon Modal 
angular.module('shoutModule').controller('ModalCoupon', function ($scope, $uibModal, $log) {
  $scope.items = ['item1', 'item2', 'item3'];
  $scope.animationsEnabled = true;
  $scope.emailBlur = false;
  $scope.passBlur = false;
  $scope.openCoupon = function (size) {
setTimeout(function(){ 
  	angular.element( document.getElementsByClassName("modal-dialog"));
  },100); 
    var modalInstance = $uibModal.open({
      animation: $scope.animationsEnabled,
      templateUrl: 'modal/coupon.html',
      controller: 'ModalInstanceCtrl',
      size: size,
      resolve: {
        items: function () {
          return $scope.items;
        }
      }
    });
    modalInstance.result.then(function (selectedItem) {
    $scope.selected = selectedItem;
    }, function () {
      $log.info('Modal dismissed at: ' + new Date());
    });
  };
  $scope.toggleAnimation = function () {
    $scope.animationsEnabled = !$scope.animationsEnabled;
  };
});

angular.module('shoutModule').controller('customjsController', function($scope){
	$scope.showCustom = function(){
		if(angular.element(document.getElementsByClassName("hideCustomInfo") ).hasClass('showCustomInfo'))
		{
			angular.element( document.getElementsByClassName("hideCustomInfo") ).removeClass('showCustomInfo');
		}
		else
		{
			angular.element( document.getElementsByClassName("hideCustomInfo") ).addClass('showCustomInfo');
		}
	}	
});
  
