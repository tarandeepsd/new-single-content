angular.module('shoutModule').directive('divAdjustHeight', ['$document', '$window', '$interval','$timeout', function( $document, $window, $interval, $timeout    ){
  return {
        scope: true,
        controller: function( $scope ){
      	},
        link: function(scope, element){ 
			console.log(angular.element(element).height()) 
			console.log(angular.element(element).height()-139)
			console.log(angular.element(element).find('.discussion-text'));
			angular.element(element).find('.discussion-text').height(angular.element(element).height()-159)

	   }
    };
  
}]);

////////////////////////////////////////////////////////////////////////////
// Pod Item Height fixed (Need to Change with Angular ( programming point))
////////////////////////////////////////////////////////////////////////////
$(function() {
    $('a[href*="#"]:not([href="#"])').click(function() {
      if (location.pathname.replace(/^\//,'') == this.pathname.replace(/^\//,'') && location.hostname == this.hostname) {
        var target = $(this.hash);
        target = target.length ? target : $('[name=' + this.hash.slice(1) +']');
		topOffset = target.offset().top;
        if (target.length) {
          $('html, body').animate({
            scrollTop: topOffset - 100
          }, 1000);
		  console.log(target.offset(),"asd");
          return false;
        }
      }
    });


  });
  
  
  
  
//////////////////////////////////////////////////////////////////////////////////
//// Pod Item Height fixed (Need to Change with Angular ( programming point))////
////////////////////////////////////////////////////////////////////////////////


$(window).resize(function(){
	jQuery('.pod-section .pod-item .image').each(function() { var $this = $(this), w = $this.find('img').width(); $this.find('img').height(w); });
	jQuery('.pod-section .pod-item.viewall-item .block a').css({'height': ((jQuery('.pod-section .product-block .pod-item.first').height()) - 0)+'px'});
	jQuery('.pod-section .pod-item.pod-height .block').css({'height': ((jQuery('.pod-section .product-block .pod-item.first .block').outerHeight()) - 0)+'px'});	
});


$(window).on('scroll',function(){
	jQuery('.pod-section .pod-item .image').each(function() { var $this = $(this), w = $this.find('img').width(); $this.find('img').height(w); });
	jQuery('.pod-section .pod-item.viewall-item .block a').css({'height': ((jQuery('.pod-section .product-block .pod-item.first').height()) - 0)+'px'});
	jQuery('.pod-section .pod-item.pod-height .block').css({'height': ((jQuery('.pod-section .product-block .pod-item.first .block').outerHeight()) - 0)+'px'});

$('body .panelpod-item .text').on('click','span',function($scope){
	$scope.stopPropagation();
});

});

jQuery('.pod-section .pod-item.pod-height .block').css({'height': ((jQuery('.pod-section .product-block .pod-item.first .block').outerHeight()) - 0)+'px'});
jQuery('.pod-section .pod-item.pod-height .block').css({'height': ((jQuery('.pod-section .product-block .pod-item.first .block').outerHeight()) - 0)+'px'});
	

setTimeout(function(){ 
	jQuery('.pod-section .pod-item.pod-height .block').css({'height': ((jQuery('.pod-section .product-block .pod-item.first .block').outerHeight()) - 0)+'px'});
	jQuery('.pod-section .pod-item.viewall-item .block a').css({'height': ((jQuery('.pod-section .product-block .pod-item.first').height()) - 0)+'px'});

},5000);




setInterval(function(){ 
 },1000);


