////////////////////////////////////////////////////////////////////////////
// CUstom Scroll Bar
////////////////////////////////////////////////////////////////////////////
angular.module('shoutModule').directive('scrollerHeight', ['$document', '$window', '$interval','$timeout', function( $document, $window, $interval, $timeout    ){
  return {
        scope: true,
        controller: function( $scope ){
      	   $scope.scrollerToggle = false;
      	   $scope.callHeight = {};
      	   $scope.isOpen = false;
      	},
        link: function(scope, element){ 
      	  var collDivHwight = true,OCollDivHwight=true;
      	  var offset =  angular.element( element ).find(">a").innerHeight();
      	  var windowHeight = angular.element(window).innerHeight(); 
      	  var divHeight = angular.element( element ).find(".drop_down_menu").innerHeight();
          scope.callHeight = {"max-height":(windowHeight-offset)+'px'};
          scope.callHeightConfig = {};
      	  /** on resize **/

      	  angular.element(window).on('resize', function(){
            windowHeight = angular.element(window).innerHeight(); 
			if (angular.element( element ).find(".mCustomScrollBox")) {
				divHeight = angular.element( element ).find(".mCustomScrollBox").scrollHeight;
				if( divHeight+ offset > windowHeight  && 0){
				  scope.callHeightConfig = {"setHeight":(windowHeight-offset)+'px'};
				  scope.callHeight = {"height":(windowHeight-offset)+'px'};
				}else{
				  scope.callHeight = {"max-height":(windowHeight-offset)+'px'};
				  scope.callHeightConfig = {};
				}
				scope.$apply();
			}
          });
	    
	    /** Click outside of document **/
	      angular.element( $document ).on('click', function( evt ){
    	      if(!(angular.element(evt.target).closest(element).length )) {
          		angular.element( element ).removeClass('open');
          		angular.element( element ).find("*").removeClass('open');
          		scope.scrollerToggle = false;
    	      }
    	    });
	    
	      var toggleClass = function( event, obj ){
  	      windowHeight = angular.element(window).innerHeight(); 
          scope.callHeight = {"max-height":(windowHeight-offset)+'px'};
          scope.$apply();
  	      angular.element( obj ).closest('li').find("*").removeClass('open');
  	      angular.element( obj ).closest('li').toggleClass('open');
  	      var clickObject = angular.element( element )
  	      .find('#scroller_category');
  	      var categary = true;
  	      if ( clickObject.length == 0 ) {
        		clickObject = angular.element( element );
        		categary = false;
  	      }
  	      var hasClassElement = clickObject.hasClass( "open" );
  	      angular.element( obj ).parent().siblings().each(function(index, value){
    		    angular.element( value ).removeClass('open');
    	      angular.element( value ).find("*").removeClass('open');
  	      });
          
	       }
	    
	 
	    
        angular.element( element ).siblings().on('click',function(){
          angular.element( element ).removeClass('open');
          angular.element( element ).find("*").removeClass('open');
        });
        angular.element( element ).find(".dropdown-toggle").on('click', function( event ){
          toggleClass(event, this );
        });
	    
	   
	    
	   }
    };
  
}]);

////////////////////////////////////////////////////////////////////////////
// Bootstrap Banner Slider
////////////////////////////////////////////////////////////////////////////
angular.module('shoutModule').controller('CarouselDemoCtrl', function ($scope) {
  $scope.myInterval = 12000;                        
  $scope.noWrapSlides = false;
 // $scope.active = 0;
  var slides = $scope.slides = [];
  var currIndex = 0;

  $scope.addSlide = function() {
    var newWidth = + slides.length + 1;  
    slides.push({
      image: 'assets/images/banner/shout_carousel_' + newWidth + '.png',
      heading: ['Know <u>when</u> to buy!','Only <u>real</u> deals'][slides.length % 2],
	    headingText: ['Discover the best buys, every single day','Shout recommends only true convenience'][slides.length % 2],
	    text: [][slides.length % 2],
      id: currIndex++
    });
  };
              
  $scope.randomize = function() {
    var indexes = generateIndexesArray();
    assignNewIndexesToSlides(indexes);
  };

  for (var i = 0; i < 2; i++) {
    $scope.addSlide();
  }

  // Randomize logic below

  function assignNewIndexesToSlides(indexes) {
    for (var i = 0, l = slides.length; i < l; i++) {
      slides[i].id = indexes.pop();
    }
  }

  function generateIndexesArray() {
    var indexes = [];
    for (var i = 0; i < currIndex; ++i) {
      indexes[i] = i;
    }
    return shuffle(indexes);
  }
  function shuffle(array) {
    var tmp, current, top = array.length;

    if (top) {
      while (--top) {
        current = Math.floor(Math.random() * (top + 1));
        tmp = array[current];
        array[current] = array[top];
        array[top] = tmp;
      }
    }

    return array;
  }
});

