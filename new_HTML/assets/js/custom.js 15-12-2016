angular.module('shoutModule', [])
angular.module('shoutModule', ['ngAnimate','ui.bootstrap', 'ngSanitize', 'ui.bootstrap-slider','nouislider', 'ngScrollbars']);
angular.module('shoutModule').directive('activeFunction',function(){
	
	return {
		restrict : "A",
		link:function(scope,element,attrs){
			console.log("element:",element);
			console.log("attrs:",attrs);

			console.log("attrs.modal:",attrs.modal);
			console.log("attrs.activeValue:",attrs.activeValue);
				if(attrs.modal === attrs.activeValue)
				{	console.log("conditon true");
					element.addClass('active');
				}
		
		}
	};
});
angular.module('shoutModule').controller('DropdownCtrl', function ($scope, $rootScope, $log, $document) {
	/* error is here in $scope.active function */
  $scope.active = function( value, modal ) {
	
    return (modal==value?'active':' ')
  }
  $scope.toggleDropdown = function() {
	  $scope.$emit('content.changed');
  }
  $scope.load_more_button=false;
  
});

angular.module('shoutModule').controller('blogCtrl', function ($scope, $rootScope, $log, $document) {

  $scope.openBlog = function()
  {
	  console.log("in open blog fun");
	  $('#first_blog').addClass('open');	
  }
  $scope.closeBlog = function()
  {
	  console.log("in close blog fun");
	  $('#first_blog').removeClass('open');
  }
  
});

angular.module('shoutModule').controller('productListCtrl', function ($scope, $rootScope, $log, $document) {
	$scope.btnText = 'Show';
	$scope.hideshow = function(btnText) {
		if(btnText == 'Show')
		{
			$scope.btnText = 'Hide';
			$(".info-address").removeClass("fadein").addClass("fadeOut");
		}
		else
		{
			$scope.btnText = 'Show';
			$(".info-address").removeClass("fadeOut").addClass("fadein");
		}
		
	};
	
	$scope.showdislike = function(id){
		angular.element('#'+id+' .outer_like_dislike').css('display','none');
		angular.element('#'+id+' .btn_dislike').css('display','block');
	}
	
	$scope.showlike = function(id){
		angular.element('#'+id+' .outer_like_dislike').css('display','none');
		angular.element('#'+id+' .btn_like').css('display','block');
	}

  $scope.ResizeWindow = function(){
    angular.element('#one .pod-section .pod-item .image').each(function() { var w = jQuery('#tabswidth').val(); console.log('scroll',w); $(this).find('span').css('height',w)});
    angular.element('#two .pod-section .pod-item .image').each(function() { var w = jQuery('#tabswidth').val(); $(this).find('span').css('height',w)});
    angular.element('#three .pod-section .pod-item .image').each(function() { var w =jQuery('#tabswidth').val(); $(this).find('span').css('height',w)});
    angular.element('#four .pod-section .pod-item .image').each(function() { var w =jQuery('#tabswidth').val(); $(this).find('span').css('height',w)});
    angular.element('#one .pod-section .pod-item .image.image-circle').each(function() { var w = $(this).width(); $(this).find('span').css('height','');$(this).find('span img').css('height',w)});
angular.element('#two .pod-section .pod-item .image.image-circle').each(function() { var w = $(this).width(); $(this).find('span').css('height','');$(this).find('span img').css('height',w)});
angular.element('#three .pod-section .pod-item .image.image-circle').each(function() { var w = $(this).width(); $(this).find('span').css('height','');$(this).find('span img').css('height',w)});
angular.element('#four .pod-section .pod-item .image.image-circle').each(function() { var w = $(this).width(); $(this).find('span').css('height','');$(this).find('span img').css('height',w)});
/*angular.element('#one .pod-section .pod-item.pod-height .block').css({'height': '347px'});
angular.element('#two .pod-section .pod-item.pod-height .block').css({'height': ((jQuery('#two .pod-section .product-block .pod-item.first .block').outerHeight()) - 0)+'px'});
angular.element('#three .pod-section .pod-item.pod-height .block').css({'height': ((jQuery('#three .pod-section .product-block .pod-item.first .block').outerHeight()) - 0)+'px'});
angular.element('#four .pod-section .pod-item.pod-height .block').css({'height': ((jQuery('#four .pod-section .product-block .pod-item.first .block').outerHeight()) - 0)+'px'});
*/  }

});

