.scrolltop, .scrollbox, .scrollbottom { position:relative}

.scrolltop:before { background:url(../../images/scrolltop.png) no-repeat top center; background-size:95%; content:" "; position:absolute; z-index:999; top:0; left:0; width:100%; height:10px;}
.scrollbox:before { background:url(../../images/scrolltop.png) no-repeat top center; background-size:95%; position:absolute; content:" "; width:100%; height:10px; top:0; left:0; right:0;  z-index:999 }
.scrollbox:after { background:url(../../images/scrollbottom.png) no-repeat bottom center; background-size:95%; position:absolute; content:" "; width:100%; height:10px; bottom:0; left:0; right:0;  z-index:999 }
.scrollbottom:before { background:url(../../images/scrollbottom.png) no-repeat bottom center; background-size:95%; bottom:0; left:0; width:100%; height:10px; content:" "; position:absolute; z-index:999 }
