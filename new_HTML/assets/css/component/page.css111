.search-result-msg h3 { color: #0050a0; font-size: 30px; font-weight: 600; margin-bottom: 23px; text-align: center; }
.search-result-msg ul { margin-right: 190px; }
.search-result-msg ul li { background-image: url(../../images/icons/disc-img.png); background-repeat: no-repeat; background-position: left 12px; color: #878787; margin: 1px 0px; font-size: 18px; padding: 0 0 0 12px; line-height: 30px; }
.search-result-msg .search-form { margin-top: 30px; }
.search-result-msg .search-deal { float: left; }
.search-result-msg .search-deal .dropdown-deal { margin: 0px; }
.search-result-msg .search-deal .dropdown-deal.open { background-color: transparent; border: 0px solid #ebebeb; border-radius: 0; box-shadow: none; }
.search-result-msg .search-deal .dropdown-deal .dropdown-toggle { padding: 0px 41px 0px 8px; border-radius: 1.5px 0px 0px 5px; border-right: 0; height: 56px; margin: 0; line-height: 56px; }
.search-result-msg .search-deal .dropdown-deal.open .dropdown-toggle { border-radius: 1.5px 0px 0px 0px; }
.search-result-msg .search-deal .dropdown-deal .dropdown-menu { left: 0; margin-top: -4px; }
.search-result-msg .search-deal .dropdown-deal .dropdown-menu li { background-image: none; padding: 0; }
.search-result-msg .search-deal .dropdown-deal .dropdown-menu::before { display: none; }
.search-result-msg .search-form .form-group { overflow: hidden; position: relative; margin: 0; }
.search-result-msg .search-form .form-group .form-input { border-radius: 0 5px 5px 0; }
.search-result-msg .search-form .form-group .form-input .form-control { height: 56px; box-shadow: none; border: 1px solid #ebebeb; font-size: 13px; border-radius: 0px; }
.search-result-msg .search-form .form-group .btn-search { position: absolute; top: 0; height: 56px; right: 0; border: 0; border-radius: 0 5px 5px 0; width: 56px; text-align: center; }
.search-result-msg .search-form .form-group .btn-search img { float: none; display: inline-block; margin: 8px 0 0 0; }
.search-podlist .view-btn { float: left; width: 100%; margin: 30px 0px 34px; }
.search-podlist .view-btn a { font-size: 20px; height: 56px; padding: 0px 20px; line-height: 56px; min-width: 280px; }
.search-result-msg .hidden-dekstop { display: none; }
/**** //Search Result message pages ****/


	
.pod-loading { text-align: center; padding: 20% 0 10%; }
.pod-loading img { vertical-align: middle; }
/** Static Sidebar **/
.filter-section .filter-block.staticSidebar > ul > li { padding: 20px 0px 16px; }
.filter-section .filter-block.staticSidebar li { font-size: 15px; }
.filter-section .filter-block.staticSidebar li a:hover { color: #0050a0; }
.filter-section .filter-block.staticSidebar li.active a { font-weight: 600; }
.filter-section .filter-block.staticSidebar ul li > .dropdown-toggle { color: #0050a0; font-size: 15px; }
.filter-section .filter-block.staticSidebar > ul > li:first-child { padding-top: 11px; }
.filter-section .filter-block.staticSidebar > ul > li:nth-child(2) { padding-top: 20px; }
.filter-section .filter-block.staticSidebar .dropdown-block ul { padding-left: 10px; }
.filter-section .filter-block.staticSidebar .dropdown-block ul li { font-size: 13px; padding: 9px 0 7px; }
.filter-section .filter-block.staticSidebar .dropdown-block ul li:last-child { padding-bottom: 1px; }
/*** Feedback Button **/
.feedback_btn { position: fixed; bottom: 6px; right: 20px; z-index: 999; opacity: 0.8; }
.feedback_btn a span { display: block; background: url(../../images/svg/feedback/feedback_button_normal.svg); width: 140px; height: 34px; }
.feedback_btn a:hover span { background: url(../../images/svg/feedback/feedback_button_onhover.svg) }
.feedback_btn a:focus span, .feedback_btn a:active span { background: url(../../images/svg/feedback/feedback_button_onclick.svg) }
.feedback_btn:hover, .feedback_btn:focus, .feedback_btn:active { opacity: 1; }
.not_cursor { cursor: auto !important; }
.feed-notification a:hover, .feed-notification:hover a { text-decoration: none; }
/*----- Fillter button hover -----------------------*/
.keyword-list li a { padding: 8px 0 8px 10px; }
.mobile-bottom-single-collection { position: fixed; bottom: 0; left: 0; width: 100%; background: #fff; padding: 15px 0; z-index: 99; }
.mobile-bottom-single-collection ul { width: 100%; float: left; }
.mobile-bottom-single-collection ul li { display: inline-block; float: left; width: 50%; text-align: center; }
.mobile-bottom-single-collection ul li:first-child { border-right: 1px solid #ddd; }
.mobile-bottom-single-collection ul li a { display: inline-block; }
.mobile-bottom-single-collection ul li a img { vertical-align: middle; }

@media (max-width:991px) {
.product-section.newdesign { clear: both; }
.newdesign.single-collection { width: 100%; }
.newdesign.single-collection .filter-section { width: 100%; float: left; }
}

@media (max-width:767px) {
.search-result-msg img { margin: 10px auto 20px; float: none; display: block; }
.search-result-msg ul { margin-right: 0; }
.feedback_btn a span { background: url(../../images/svg/feedback/feedback_normal.svg); width: 34px; }
.feedback_btn a:hover span { background: url(../../images/svg/feedback/feedback_onhover.svg) }
.feedback_btn a:focus span, .feedback_btn a:active span { background: url(../../images/svg/feedback/feedback_onclick.svg) }
}
